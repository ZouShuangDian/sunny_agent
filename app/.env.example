# Database
# Format: postgresql+asyncpg://user:password@host:port/database
DATABASE_URL=postgresql+asyncpg://USER:PASSWORD@HOST:PORT/DB
# Schema used by the app
DB_SCHEMA=sunny_agent

# Redis
# Format: redis://:password@host:port/db
REDIS_URL=redis://:PASSWORD@HOST:PORT/DB

# JWT
JWT_SECRET=CHANGE_ME
JWT_ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=180
REFRESH_TOKEN_EXPIRE_MINUTES=10080

# LLM (OpenAI-compatible API)
LLM_DEFAULT_MODEL=openai/deepseek-ai/DeepSeek-V3
LLM_API_KEY=CHANGE_ME
LLM_API_BASE=https://YOUR_LLM_BASE/v1
LLM_TIMEOUT=60

# Bocha Web Search
BOCHA_API_KEY=CHANGE_ME

# Milvus Vector DB
MILVUS_URI=http://HOST:19530
MILVUS_DB=sunny_agent
MILVUS_USER=root
MILVUS_PASSWORD=CHANGE_ME

# Embedding API (bge-m3)
EMBEDDING_API_BASE=https://YOUR_EMBEDDING_BASE
EMBEDDING_API_KEY=CHANGE_ME
EMBEDDING_MODEL=BAAI/bge-m3
EMBEDDING_DIM=1024
EMBEDDING_TIMEOUT=30

# Rerank API (bge-reranker-v2-m3)
RERANK_API_BASE=https://YOUR_RERANK_BASE
RERANK_API_KEY=CHANGE_ME
RERANK_MODEL=Bge-reranker-v2-m3

# App
ENV=development
APP_NAME=agent-sunny
APP_PORT=8010
